extend jade-templates/main.jade
block head
    - var title         =   "Home Page";
    - var description   =   "Home Page";
    - var keywords      =   "";
    - var author        =   "Nicholas J. Phillips";
    ///Metadata
    meta(charset="UTF-8")
    meta(name="description", content=description)
    meta(name="keywords", content=keywords)
    meta(name="author", content=author)
    //\Metadata
    
    block other

    ///Title
    title=title
    //\Title
block body
    .row
        .col-md-10
            .container
        .col-md-2.widgets
            link(rel="stylesheet", type="text/css", href="http://tenk77.github.io/css/widgets-col.css")
            script(type="application/javascript", src="http://tenk77.github.io/js/sticky.min.js")
            include widgets/contacts.jade
            include widgets/links.jade
            script(type="application/javascript").
                setTimeout(function() {
                    $(".widgets").stickySidebar({
                        timer: 1,
                        padding: 30
                    });
                }, 1 * 1000);
    .row
        .col-md-12
            - for(var i = 0; i < 100; i++)
                br